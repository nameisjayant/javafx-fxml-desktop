<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.text.Font?>

<VBox alignment="TOP_CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.nameisjayant.demo.start.StartController" prefHeight="400.0" prefWidth="600.0">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <Label fx:id="title" text="Welcome to NCIRS" style="-fx-color: #1e2b3c">
        <font>
            <Font size="35.0"/>
        </font>
    </Label>
    <Button layoutX="264.0" layoutY="504.0" onAction="#login" prefHeight="50.0" prefWidth="163.0"
            style="-fx-background-color: #1e2b3c"
            text="Login" textAlignment="CENTER" textFill="WHITE">
        <font>
            <Font name="System Bold" size="16.0"/>
        </font>
    </Button>
    <Button layoutX="264.0" layoutY="504.0" onAction="#register" prefHeight="50.0" prefWidth="163.0"
            style="-fx-background-color: #1e2b3c"
            text="Register" textAlignment="CENTER" textFill="WHITE">
        <font>
            <Font name="System Bold" size="16.0"/>
        </font>
    </Button>
</VBox>
